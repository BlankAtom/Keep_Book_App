<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleDot">
        <attr name="inner_color" format="color" />
        <attr name="stroke_color" format="color" />
        <attr name="ring_color" format="color" />
        <attr name="able_select" format="boolean" />
        <attr name="selected" format="boolean" />
        <attr name="ring" format="boolean" />
        <attr name="radius" format="float" />
        <attr name="stroke_ratio" format="float" />
    </declare-styleable>


    <declare-styleable name="MaterialNumberPicker">
        <attr name="mnpDefaultValue" format="integer" />
        <attr name="mnpMinValue" format="integer" />
        <attr name="mnpMaxValue" format="integer" />
        <attr name="mnpTextSize" format="dimension" />
        <attr name="mnpTextColor" format="color" />
        <attr name="mnpTextStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
            <flag name="bold_italic" value="3" />
        </attr>
        <attr name="mnpFontname" format="string" />
        <attr name="mnpSeparatorColor" format="color" />
        <attr name="mnpEditable" format="boolean" />
        <attr name="mnpWrapped" format="boolean" />
    </declare-styleable>
</resources>